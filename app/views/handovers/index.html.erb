<h1>All handovers</h1>

<table>

    <thead>
        <tr>
            <th>Activity</th>
            <th>Blob rate</th>
            <th>Ejection rate</th>
            <th>Pending issues</th>
            <th>status</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @handovers.each do |handover| %>
            <tr>
                <td><%= handover.activities %></td>
                <td><%= handover.blob_rates %></td>
                <td><%= handover.ejection_rates %></td>
                <td><%= handover.pending_issues %></td>
                <td><%= handover.status %></td>
                <td><%= link_to 'Show', handover %></td>
                <td><%= link_to 'Edit', edit_handover_path(handover) %></td>
                <td><%= link_to 'Destroy', handover, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %>
    </tbody>

</table>

<br>

<%= link_to 'New Handover', new_handover_path %>

